{
    "name": "midorikocak/view",
    "type": "library",
    "description": "View is the V in MVC. A small library to include in your MVC projects.",
    "keywords": [
        "midorikocak",
        "view"
    ],
    "homepage": "https://github.com/midorikocak/view",
    "license": "MIT",
    "authors": [
        {
            "name": "Midori Kocak",
            "email": "mtkocak@gmail.com",
            "homepage": "https://www.mtkocak.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "ext-json": "*",
        "eftec/bladeone": "^3.34"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.5",
        "opsway/psr12-strict-coding-standard": "^0.3.0",
        "phpunit/phpunit": ">=8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/process": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "midorikocak\\view\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "midorikocak\\view\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
